  "files": [
    "info.plist",
    "*.js",
    "*.scpt",
    "*.sh",
    "icon.png",
    "imgs/*.png",
    "icon/*.png",
    "initialCachingDescription.html",
    "searchConfig.json",
    "*.md",
    "LICENSE",
    "arvis-workflow.json"
  ],
